<!DOCTYPE HTML>
<html>

<head>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
  <title>Zeraflix by BernardJeremy</title>
  <link rel="icon" type="image/jpg" href="/images/favicon.png" />
  <link rel="stylesheet" type="text/css" href="/css/style.css">
  <link rel="stylesheet" type="text/css" href="/css/pagination.css">
</head>

<body>
  <div class="wrap">
    <div class="sidebar">
      <header>
        <div class="logo">
          <a href="<%= url('home') %>" title="Home">
            <img src="/images/zera-logo.png"/>
          </a>
        </div>
        <div class="clearFloat"> </div>
      </header>
      <div class="clearFloat"></div>
    </div>
    <div class="content">
      <div class="clearFloat"></div>
      <div class="part">
        <h2>
         Zeraflix - Page 1 <span class="loading-message">(Loading...)</span>
        </h2>
        <span class="part-bottom"></span>
        <div class="clearFloat"></div>
        <section class="container">
          <nav class="pagination">
            <a id="link-prev" href="#" data-offset="0" class="prev">&lt;</a>
            <a id="link-next" href="#" data-offset="0" class="next">&gt;</a>
          </nav>
        </section>
      </div>
      <footer>
        <div class="clearFloat"></div>
        <div class="copy">
          <p>&copy; Zeraflix - All Rights Reserved</a></p>
        </div>
      </footer>
    </div>
    <div class="clearFloat"></div>
  </div>

  <script src="/jquery/jquery.js"></script>
  <script src="/ejs/ejs.min.js"></script>

  <script>
    var client_id = '<%= client_id %>';
    var redirect_uri = '<%= redirect_uri %>';
  </script>

  <script src="/js/handleVideo.js"></script>  

</body>

</html>